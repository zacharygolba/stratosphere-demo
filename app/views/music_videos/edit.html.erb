<div class="container">
    <div class="content-heading clearfix">
        <div class="pull-left">
            <h4>Music Video: <%= @music_video.id %></h4>
        </div>
        <div class="pull-right">
            <div class="btn-group btn-group-xs">
                <button type="button" class="btn btn-primary" data-perform="back">Back</button>
                <button type="button" class="btn btn-primary" data-perform="delete" data-model="music_video" data-id="<%= @music_video.id %>">Delete</button>
            </div>
        </div>
    </div>
    <div class="form-group">
        <%= render partial: 'stratosphere/attachment_field', locals: { model: @music_video } %>
    </div>
</div>